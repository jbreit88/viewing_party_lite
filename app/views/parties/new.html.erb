<%= form_with model: @party, local: true do |f|%>
<%= f.label :duration %>
<%= f.text_field :duration %>
<%= f.label :when %>
<%= f.text_field :when %>
<%= f.label :start_time %>
<%= f.text_field :start_time %>
<% @users.each do |user| %>
  <%= f.check_box :to, user.name %>
<% end %>
<%= f.submit "Create Party" %>
<% end %>
