<%= form_with model: @user, local: true do |f|%>
<%= f.label :duration %>
<%= f.text_field :duration %>
<%= f.label :when %>
<%= f.text_field :when %>
<%= f.label :start_time %>
<%= f.text_field :start_time %>
<p><% @users.each do |user| %>
  <%= f.check_box user %>
  <%= f.label user.name %>
<% end %></p>
<%= f.submit "Create Party" %>
<% end %>
